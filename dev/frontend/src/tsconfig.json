{
      "compilerOptions": {
            "module": "ESNext",
            "target": "ESNext",
            "moduleResolution": "node",
            "jsx": "react-jsx",
            "outDir": "../../src/main/resources/static/js/",
            "rootDir": "./",
            "esModuleInterop": true,
            "strict": true,
            "forceConsistentCasingInFileNames": true,
            "skipLibCheck": true
      },
      "include": ["**/**/*.ts"],
      "exclude": ["node_modules"]
}
